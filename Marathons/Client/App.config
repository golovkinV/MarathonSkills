<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAdminService" />
                <binding name="BasicHttpBinding_IUserService" />
                <binding name="BasicHttpBinding_IRunnerService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/AdminService/Service1/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAdminService"
                contract="AdminService.IAdminService" name="BasicHttpBinding_IAdminService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/UserService/Service1/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUserService"
                contract="UserService.IUserService" name="BasicHttpBinding_IUserService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/RunnerService/Service1/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRunnerService"
                contract="RunnerService.IRunnerService" name="BasicHttpBinding_IRunnerService" />
        </client>
    </system.serviceModel>
</configuration>